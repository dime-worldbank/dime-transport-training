@article{Vittori2021,
  title = {Threat or Corrective? {{Assessing}} the Impact of Populist Parties in Government on the Qualities of Democracy: {{A}} 19-{{Country}} Comparison},
  author = {Vittori, Davide},
  year = {2021},
  month = jul,
  journal = {Government and Opposition},
  pages = {1--21},
  issn = {0017-257X},
  doi = {10.1017/gov.2021.21},
  abstract = {\textexclamdown p\textquestiondown Scholars have long debated whether populism harms or improves the quality of democracy. This article contributes to this debate by focusing on the impact of populist parties in government. In particular, it inquires: (1) whether populists in government are more likely than non-populists to negatively affect the quality of democracies; (2) whether the role of populists in government matters; and (3) which type of populism is expected to negatively affect the quality of liberal-democratic regimes. The results find strong evidence that the role of populists in government affects several qualities of democracy. While robust, the findings related to (2) are less clear-cut than those pertaining to (1). Finally, regardless of their role in government, different types of populism have different impacts on the qualities of democracy. The results show that exclusionary populist parties in government tend to have more of a negative impact than other forms of populism.\textexclamdown/p\textquestiondown}
}

