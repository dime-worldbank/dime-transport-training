  file = {C\:\\Users\\bjorn\\Zotero\\storage\\BISNCQSR\\Ambec et al_2013_The Porter Hypothesis at 20.pdf;C\:\\Users\\bjorn\\Zotero\\storage\\EGNYK7DV\\Ambec et al. - 2013 - The Porter Hypothesis at 20 Can Environmental Reg.pdf}
