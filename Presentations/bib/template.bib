  file = {C\:\\Users\\bjorn\\Zotero\\storage\\IVSV5DDP\\Jackson_2014_Networks in the Understanding of Economic Behaviors.pdf;C\:\\Users\\bjorn\\Zotero\\storage\\S7J5PH65\\articles.html}
