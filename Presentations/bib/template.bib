@article{golubNetworkStructureSpeed2012,
  title = {Network {{Structure}} and the {{Speed}} of {{Learning Measuring Homophily Based}} on Its {{Consequences}}},
  author = {Golub, Benjamin and Jackson, Matthew O.},
  year = {2012},
  journal = {Annals of Economics and Statistics},
  number = {107/108},
  pages = {33--48},
  publisher = {{[GENES, ADRES]}},
  issn = {2115-4430},
  doi = {10.2307/23646571},
  abstract = {Homophily is the tendency of people to associate relatively more with those who are similar to them than with those who are not. In Golub and Jackson [2012], we introduced degree-weighted homophily (DWH), a new measure of this phenomenon, and showed that it gives a lower bound on the time it takes for a certain natural best-reply or learning process operating in a social network to converge. Here we show that, in important settings, the DWH convergence bound does substantially better than previous bounds based on the Cheeger inequality. We also develop a new complementary upper bound on convergence time, tightening the relationship between DWH and updating processes on networks. In doing so, we suggest that DWH is a natural homophily measure because it tightly tracks a key consequence of homophily \textemdash{} namely, slowdowns in updating processes. L'homophilie est la tendance qu'ont les individus \`a fr\'equenter davantage ceux qui leur ressemblent que ceux qui sont diff\'erents. Dans Golub et Jackson (2011), nous avions introduit une nouvelle mesure de ce ph\'enom\`ene : l'homophilie pond\'er\'ee par le degr\'e (degree-weighted homophily : DWH). Nous avions d\'emontr\'e que cette mesure offrait une borne inf\'erieure du temps n\'ecessaire \`a la convergence de certains processus de meilleure r\'eponse ou de diss\'emination d'information qui \'evoluent dans un r\'eseau social. Ici, nous d\'emontrons que, dans des situations dignes d'int\'er\^et, la DWH fournit une borne inf\'erieure bien meilleure que des constructions ant\'erieures bas\'ees sur l'in\'egalit\'e de Cheeger. Nous d\'eveloppons \'egalement une borne sup\'erieure du temps n\'ecessaire \`a la convergence, ce qui renforce les liens entre la DWH et les processus d'actualisation sur les r\'eseaux. Nous sugg\'erons ainsi que la DWH est une mesure naturelle de l'homophilie parce qu'elle en capture avec pr\'ecision une cons\'equence essentielle : les ralentissements dans les processus d'actualisation (ou encore : les ralentissements de circulation de l'information).}
}

